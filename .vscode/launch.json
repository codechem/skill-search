  "version": "0.2.0",
  "configurations": [
    {
      "name": "Fill DB",
      "type": "debugpy",
      "request": "launch",
      "module": "skill_search.fill_db"
    },
    {
      "name": "Main",
      "type": "debugpy",
      "request": "launch",
